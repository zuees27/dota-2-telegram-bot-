datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

model User {
  id         Int      @id @default(autoincrement())
  telegramId String   @unique
  username   String?
  createdAt  DateTime @default(now())
}